// The width of the pulse sent to the left solenoid, in 32us units.
BITCODEC_DATA_U(uint8_t, width1, 0, 7, 0)

// The width of the pulse sent to the right solenoid, in 32us units.
BITCODEC_DATA_U(uint8_t, width2, 8, 7, 0)

// True -> right solenoid fires autokick_offset time after left solenoid. False -> left solenoid fires autokick_offset time after right solenoid.
BITCODEC_DATA_BOOL(offset_sign, 16, false)

// The time difference between the start of the left solenoid pulse and the start of the right solenoid pulse, in 32us units.
BITCODEC_DATA_U(uint8_t, offset, 17, 7, 0)
